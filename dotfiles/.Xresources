Emacs.FontBackend: xft
Emacs.foreground: grey89
Emacs.background: black
Emacs.cursorColor: rgb:60/95/85
Emacs.pointerColor: grey30
Emacs.borderColor: grey30
Emacs.Font: DejaVu Sans Mono-9
Emacs.*.*.*.marginHeight: 1
Emacs.*.*.*.background: grey60
Emacs.*.*.*.foreground: white
Emacs.internalBorder: 0
Emacs.BorderWidth: 0

Emacs.menuBar: off
Emacs.verticalScrollBars: off
Emacs.toolBar: 0

URxvt.font: xft:Inconsolata:9

! See: https://wiki.archlinux.org/index.php/Font_configuration
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintslight  ! hint{none,slight,medium,full}
! 4K display physical DPI is around 162.
Xft.dpi:       160
